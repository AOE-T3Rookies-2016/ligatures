  "authors": [
    {
      "name": "Irena",
      "role": "Developer"
    },
    {
      "name": "Willy",
      "role": "Developer"
    },
    {
      "name": "Niels",
      "role": "Developer"
    },
    {
      "name": "Boban",
      "role": "Developer"
    }
